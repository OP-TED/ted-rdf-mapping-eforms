        [
            rdfs:label  "ND-NoticeResultGroupFA";
            rr:template "http://data.europa.eu/a4g/resource/id_{replace(replace(/*/cbc:ID[@schemeName='notice-id'], ' ', '-' ), '/' , '-')}_LotGroup_{efac:TenderLot/cbc:ID}" ;
            rr:class epo:LotGroup
        ] ;
